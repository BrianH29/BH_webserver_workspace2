<properties>
	<entry key="selectNoticeList">
		SELECT /*com.kh.notice.model.dao.NoticeDao#selectNoticeList()*/
			  NOTICE_NO
			, NOTICE_TITLE
			, USER_ID
			, COUNT
			, CREATE_DATE
		 FROM NOTICE N
		 JOIN MEMBER ON (NOTICE_WRITER=USER_NO)
		WHERE N.STATUS='Y'
		ORDER
		   BY NOTICE_NO DESC
	</entry>
	
